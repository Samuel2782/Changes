{
   "action": {
      "default_title": "AI Workspace",
      "default_popup": "popup/index.html"
   },
   "background": {
      "service_worker": "serviceWorker.js",
      "type": "module"
   },
   "chrome_url_overrides": {
      "newtab": "index.html"
   },
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "content-scripts/auth-interop.js" ],
      "matches": [ "https://*.yourdomain.com/*" ],
      "run_at": "document_end"
   } ],
   "content_security_policy": {
      "extension_pages": "script-src 'self'; object-src 'self'"
   },
   "description": "Transform your browser into a beautifully designed tab that helps you feel calm, keep focus, and stay energized.",
   "icons": {
      "128": "img/meta/icon-128.png",
      "16": "img/meta/icon-16.png",
      "32": "img/meta/icon-32.png",
      "48": "img/meta/icon-48.png"
   },
   "key": "",
   "manifest_version": 3,
   "name": "Momentum",
   "offline_enabled": true,
   "optional_host_permissions": [ "file:///*", "*://*/*" ],
   "optional_permissions": [ "bookmarks", "tabs", "sessions", "topSites", "favicon", "scripting", "alarms", "notifications", "tabGroups", "search" ],
   "permissions": [ "offscreen", "unlimitedStorage", "idle" ],
   "short_name": "Momentum",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "2.25.1",
   "web_accessible_resources": [ {
      "matches": [ "*://*/*" ],
      "resources": [ "site-blocker.html" ]
   } ]
}
